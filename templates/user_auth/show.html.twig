{% extends 'base.html.twig' %}

{% block title %}UserAuth{% endblock %}

{% block body %}
    <h1>UserAuth</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ user_auth.id }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ user_auth.email }}</td>
            </tr>
            <tr>
                <th>Roles</th>
                <td>{{ user_auth.roles ? user_auth.roles|json_encode : '' }}</td>
            </tr>
            <tr>
                <th>Password</th>
                <td>{{ user_auth.password }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ user_auth.name }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('user_auth_index') }}">back to list</a>

    <a href="{{ path('user_auth_edit', {'id': user_auth.id}) }}">edit</a>

    {{ include('user_auth/_delete_form.html.twig') }}
{% endblock %}
